{
  "name": "@sruim/rhi",
  "version": "1.0.0",
  "description": "WebGL渲染层",
  "module": "./dist/index.mjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "prebuild": "pnpm clean",
    "build": "tsc",
    "build:module": "rollup -c",
    "build:declaration": "tsc -d --emitDeclarationOnly",
    "clean": "rimraf dist",
    "prepublishOnly": "pnpm build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "contributors": [
    {
      "name": "Sruimeng"
    }
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "dependencies": {
    "@sruim/core": "workspace:*",
    "@sruim/math": "workspace:*",
    "@sruim/specification": "workspace:*"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@vitejs/plugin-legacy": "^4.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "rimraf": "^5.0.0",
    "rollup": "^3.20.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.0",
    "vite-tsconfig-paths": "^4.0.0"
  }
