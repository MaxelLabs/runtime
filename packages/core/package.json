{
  "name": "@maxellabs/core",
  "version": "0.0.8",
  "description": "核心库",
  "module": "./dist/index.mjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "prebuild": "pnpm clean",
    "build": "pnpm build:types && pnpm build:js",
    "build:js": "rollup -c",
    "build:types": "tsc --build --force --emitDeclarationOnly",
    "clean": "rimraf dist",
    "prepublishOnly": "pnpm build",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "contributors": [
    {
      "name": "Sruimeng"
    }
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "dependencies": {
    "@maxellabs/math": "workspace:*",
    "@maxellabs/specification": "workspace:*"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@types/ip": "^1.1.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "@vitejs/plugin-legacy": "^7.2.1",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.0.5",
    "rimraf": "^6.1.2",
    "rollup": "^4.46.2",
    "ts-jest": "^29.4.6",
    "typescript": "^5.0.4",
    "vite": "^7.3.0",
    "vite-tsconfig-paths": "^6.0.3"
  }
}
